{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "barryvdh/laravel-cors": "^0.11.4",
        "davidhsianturi/laravel-compass": "^0.4",
        "fideloper/proxy": "^4.0",
        "hashids/hashids": "^4.0",
        "laravel/framework": "^6.7",
        "laravel/horizon": "^3.4",
        "laravel/socialite": "^4.2",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "nesbot/carbon": "^2.26.0",
        "sendgrid/sendgrid": "~7",
        "spatie/laravel-permission": "^3.3",
        "tymon/jwt-auth": "^1.0.0-rc.5",
        "uonsoftware/laravel-auth": "^0.1.3",
        "uonsoftware/rsa-signer": "^1.0.9"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master"
    },
    "repositories": [
        {
            "url": "git@github.com:BrosSquad/JWTAuth.git",
            "type": "git",
            "branch": "master"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan jwt:secret --no-interaction --force --no-ansi",
            "@php artisan storage:link",
            "@php artisan telescope:publish",
            "@php artisan horizon:assets"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
